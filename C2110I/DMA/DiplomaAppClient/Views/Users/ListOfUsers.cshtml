@{
}

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Username</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in ViewBag.AllUsers)
        {
            <tr>
                <td>@user.UsersId</td>
                <td>@user.UserName</td>
                <td>@user.Discription</td>
            </tr>
        }
    </tbody>
</table>

